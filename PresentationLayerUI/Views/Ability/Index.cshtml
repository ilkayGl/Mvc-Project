@using EntityLayer.Concrete
@model List<Ability>
@{
    ViewBag.Title = "Index";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Beceriler</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Skills Profile Widget template Responsive, Login form web template,Flat Pricing tables,Flat Drop downs  Sign up Web Templates, Flat Web Templates, Login sign up Responsive web template, SmartPhone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="~/AdminLTE-3.0.4/web/application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--Custom Theme files-->
    <link href="~/AdminLTE-3.0.4/web/css/style.css" rel="stylesheet" type="text/css" />
    <!--//Custom Theme files -->
    <!--web font-->
    <link href='//fonts.googleapis.com/css?family=Questrial' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <!--//web font-->
    <!--circle-chart-->
    <script src="~/AdminLTE-3.0.4/web/js/jquery-1.11.1.min.js"></script>
    <!--//circle-chart-->
    <!--ResponsiveTabs-->
    <script src="~/AdminLTE-3.0.4/web/js/easyResponsiveTabs.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $('#horizontalTab').easyResponsiveTabs({
                type: 'default', //Types: default, vertical, accordion
                width: 'auto', //auto or any width like 600px
                fit: true   // 100% fit in a container
            });
        });
    </script>
    <!--//ResponsiveTabs-->
</head>

<body>
    <!-- main -->
    <div class="main">
        <h1>Ben Kimim ?</h1>
        <div class="main-info w3l">
            <div class="main-row">
                <!-- main-row-one -->
                <div class="profile-grid logo wthree">
                    <img style="height:150px; width:150px;" src="~/ProjectImages/1620139567131.jpg" alt="">
                    <h2>İlkay Gül</h2>
                    <p>Yönetim Bilişim Sistemleri</p>

                </div>
                <h3 class="title">Yetenekler</h3>
                <div class="col-md-6 skills-right bar_group">
                    @foreach (var item in Model)
                    {

                        <div class='bar_group__bar thin' label='@item.AbilityName &nbsp;&nbsp; @item.AbilityCount%' value='@item.AbilityCount'></div>

                    }
                </div>
                <!--bar-js-->

                <div class="social-icons agileits">
                    <ul>

                        <li><a href="https://www.linkedin.com/in/ilkay-gül-839502170/" target="_blank" class="be"></a></li>

                    </ul>
                </div>
                <div class="clear"> </div>
            </div>
        </div>
        <!-- copyright -->
        <div class="copyright">
            <p> © 2021 İlkay Gül_ | <a href="/Heading/Index" target="_blank">Başlıklara Dön</a></p>
        </div>
        <!-- //copyright -->
    </div>
    <!-- main -->
</body>
</html>

<script src="~/AdminLTE-3.0.4/web/js/bars.js"></script>

